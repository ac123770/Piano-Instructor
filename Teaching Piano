<script type='text/javascript'>

$(document).ready(function(){

            var height = $('div.text').height();

            $('div.apps').css("height",height+160);

            $('div.content').css("height",height+260);

           

            $('div.item#home').click(function(){

                        $('div.block#left').load('links/home.html');

                        });

           

            $('div.item#about').click(function(){

                        $('div.block#left').load('links/about.html');

                        });

 

           

            $('div.item').mouseover(function(){

                        $(this).css("cursor","pointer");

                        $(this).stop();

                        $(this).animate({'margin-top':'18px'},260);

            });

           

            $('div.item').mouseout(function(){

                        $(this).stop();

                        $(this).animate({'margin-top':'0px'},350);

            });       

           

});

</script>

